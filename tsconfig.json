{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "strictNullChecks": true,
    "paths": {
      "@assets/*": ["public/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@models/*": ["src/models/*"]
    }
  }
}
